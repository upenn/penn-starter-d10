{
    "name": "pantheon-upstreams/upstream-configuration",
    "type": "project",
    "version": "dev-main",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": [
                "drupal/bg_img_field"
            ]
        },
        {
            "type": "vcs",
            "url": "https://git.drupalcode.org/issue/bg_img_field-3296632.git"
        }
    ],
    
    "require": {
        "drupal/core-recommended": "^10",
        "zaporylie/composer-drupal-optimizations": "^1.2",
        "drupal/radix": "^5.0",
        "drupal/simplesamlphp_auth": "^4.0",
        "drupal/smtp": "^1.2",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.0",
        "drupal/gin": "^3.0",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/bg_image_formatter": ">=1.12",
        "drupal/bootstrap_layouts": ">=5.1",
        "drupal/bootstrap_site_alert": "^1.12",
        "drupal/ckeditor_accordion": "^2.0",
        "drupal/ckeditor_bootstrap_buttons": "^2.0",
        "drupal/components": ">=3.0@beta",
        "drupal/structure_sync": "^2.0",
        "drupal/menu_block": ">=1.8",
        "drupal/addtocal": "^3.0@beta",
        "drupal/editoria11y": "^2.1",
        "drupal/social_media_links": "^2.8",
        "drupal/time_field": "^2.0",
        "drupal/ds": ">=3.5",
        "drupal/default_content": "^2.0@alpha",
        "drupal/mysql56": "^1.4",
        "drupal/pathauto": "^1.10",
        "drupal/webform": "^6.2",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/google_analytics": "^4.0",
        "drupal/upgrade_status": "^4.0"
    },
    "conflict": {
        "symfony/http-foundation": "3.4.24"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
    },
    "extra": {
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "zaporylie/composer-drupal-optimizations": true,
            "drupal/console-extend-plugin": true,
            "simplesamlphp/composer-module-installer": true
        }
    }
}
